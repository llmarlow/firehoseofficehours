<h1>Edit Question</h1>
<%= form_for(@question) do |f| %>
  <p>
    <%= f.label :question %><br/>
    <%= f.text_field :question %>
  </p>
  <p>
    <%= f.label :info %><br/>
    <%= f.text_area :info %>
  </p>
  <p>
    <%= f.label :link %><br/>
    <%= f.text_field :link %>
  </p>
  <p>
    <%= f.label :start_time %><br/>
    <%= f.time_field :start_time %>
  </p>
  <p>
    <%= f.label :end_time %><br/>
    <%= f.time_field :end_time %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>